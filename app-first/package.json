{
    "name": "app-first",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "nodemon": "nodemon --config nodemon.json -e js,ts,json,hbs",
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --debug --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@nestjs-modules/mailer": "^1.6.0",
        "@nestjs/common": "^8.0.0",
        "@nestjs/config": "^1.0.3",
        "@nestjs/core": "^8.0.0",
        "@nestjs/jwt": "^8.0.0",
        "@nestjs/mongoose": "^9.0.0",
        "@nestjs/passport": "^8.0.1",
        "@nestjs/platform-express": "^8.0.0",
        "@nestjs/platform-fastify": "^8.0.11",
        "@nestjs/platform-socket.io": "^8.0.11",
        "@nestjs/typeorm": "^8.0.2",
        "@nestjs/websockets": "^8.0.11",
        "bcrypt": "^5.0.1",
        "class-transformer": "^0.4.0",
        "class-validator": "^0.13.1",
        "cookie-parser": "^1.4.5",
        "express-session": "^1.17.2",
        "fastify": "^3.22.0",
        "fastify-cookie": "^5.3.1",
        "fastify-static": "^4.4.0",
        "handlebars": "^4.7.7",
        "hbs": "^4.1.2",
        "joi": "^17.4.2",
        "mongoose": "^6.0.8",
        "mysql2": "^2.3.0",
        "nodemailer": "^6.7.0",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "point-of-view": "^4.15.2",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.2.0",
        "socketio": "^1.0.0",
        "typeorm": "^0.2.38",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@nestjs/cli": "^8.0.0",
        "@nestjs/schematics": "^8.0.0",
        "@nestjs/testing": "^8.0.0",
        "@types/bcrypt": "^5.0.0",
        "@types/cookie-parser": "^1.4.2",
        "@types/express": "^4.17.13",
        "@types/express-session": "^1.17.4",
        "@types/jest": "^27.0.1",
        "@types/joi": "^17.2.3",
        "@types/multer": "^1.4.7",
        "@types/node": "^16.0.0",
        "@types/nodemailer": "^6.4.4",
        "@types/passport-jwt": "^3.0.6",
        "@types/passport-local": "^1.0.34",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^4.28.2",
        "@typescript-eslint/parser": "^4.28.2",
        "eslint": "^7.30.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "jest": "^27.0.6",
        "prettier": "^2.3.2",
        "run-script-webpack-plugin": "^0.0.11",
        "supertest": "^6.1.3",
        "ts-jest": "^27.0.3",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "^3.10.1",
        "typescript": "^4.3.5",
        "webpack": "^5.56.0",
        "webpack-cli": "^4.8.0",
        "webpack-node-externals": "^3.0.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "engines": {
        "node": "16.x"
    }
}
